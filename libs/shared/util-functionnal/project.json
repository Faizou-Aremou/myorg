{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/util-functionnal/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/shared/util-functionnal"],
      "options": {
        "project": "libs/shared/util-functionnal/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/util-functionnal/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/util-functionnal/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/util-functionnal/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/util-functionnal"],
      "options": {
        "jestConfig": "libs/shared/util-functionnal/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
