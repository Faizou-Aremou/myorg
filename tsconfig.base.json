{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@web-times-team/authorization": [
        "libs/shared/authorization/src/index.ts"
      ],
      "@web-times-team/http-services": [
        "libs/shared/http-services/src/index.ts"
      ],
      "@web-times-team/message": ["libs/shared/message/src/index.ts"],
      "@web-times-team/ui-catalog": ["libs/shared/ui-catalog/src/index.ts"],
      "@web-times-team/ui-file": ["libs/shared/ui-file/src/index.ts"],
      "@web-times-team/ui-tabs": ["libs/shared/ui-tabs/src/index.ts"],
      "@web-times-team/util-animation": [
        "libs/shared/util-animation/src/index.ts"
      ],
      "@web-times-team/util-binary-tree": [
        "libs/shared/util-binary-tree/src/index.ts"
      ],
      "@web-times-team/util-canvas": ["libs/shared/util-canvas/src/index.ts"],
      "@web-times-team/util-figure": ["libs/shared/util-figure/src/index.ts"],
      "@web-times-team/util-functionnal": [
        "libs/shared/util-functionnal/src/index.ts"
      ],
      "@web-times-team/util-media": ["libs/shared/util-media/src/index.ts"],
      "@web-times-team/util-number": ["libs/shared/util-number/src/index.ts"],
      "@web-times-team/util-sequence": [
        "libs/shared/util-sequence/src/index.ts"
      ],
      "@web-times-team/util-tree": ["libs/shared/util-tree/src/index.ts"],
      "@web-times-team/util-ui": ["libs/shared/util-ui/src/index.ts"],
      "@web-times-team/util-web-component": [
        "libs/shared/util-web-component/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
